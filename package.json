{
  "name": "history-inspector",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "build": "browserify ./public/app.js > ./public/bundle.js",
    "watch": "npm run watch-css & watchify ./public/app.js -o public/bundle.js -dv",
    "watch-css": "node-sass -w ./public/styles -o public/build/styles",
    "start": "npm run watch & ecstatic -p 8000 ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/faiq/history-inspector.git"
  },
  "author": "faiq",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/faiq/history-inspector/issues"
  },
  "homepage": "https://github.com/faiq/history-inspector#readme",
  "devDependencies": {
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "node-sass": "^3.3.3",
    "watchify": "^3.4.0"
  },
  "dependencies": {
    "ecstatic": "^1.1.3",
    "react": "^0.13.3",
    "react-redux": "^3.1.0",
    "redux": "^3.0.2"
  }
}
